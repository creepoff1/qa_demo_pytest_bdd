{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["data", "support"],
  "properties": {
    "data": {
      "type": "object",
      "required": ["id", "name", "year", "color", "pantone_value"],
      "properties": {
        "id": { "type": "integer" },
        "name": { "type": "string" },
        "year": { "type": "integer" },
        "color": { "type": "string" },
        "pantone_value": { "type": "string" }
      },
      "additionalProperties": true
    },
    "support": {
      "type": "object",
      "required": ["url", "text"],
      "properties": {
        "url": { "type": "string", "format": "uri" },
        "text": { "type": "string" }
      },
      "additionalProperties": true
    }
  },
  "additionalProperties": true
}
